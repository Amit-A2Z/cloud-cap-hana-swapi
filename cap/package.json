{
  "name": "cap-hana-swapi",
  "version": "1.1.0",
  "description": "SAP Cloud Application Programming Model fun sample to demonstrate many-to-many relationships.",
  "repository": "https://github.com/SAP-samples/cloud-cap-hana-swapi",
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@sap/cds": "^6.0.1",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.0",
    "@sap/cds-graphql": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "hdb": "^0.19.3",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "hana": "cds deploy --to hana:starwars --auto-undeploy",
    "start": "cds run",
    "build": "cds build",
    "build2": "cp ./gen/db/.env ./ && cds build && cp ./.env ./gen/db/",
    "openapi": "cds compile srv --service all -o docs --to openapi --openapi:diagram",
    "load": "node convertData",
    "changeLog": "node CHANGELOG.mjs",
    "packDb":"pack build cap-hana-swapi-db --path gen/db --builder paketobuildpacks/builder:base",
    "pack": "pack build cap-hana-swapi-srv --path gen/srv --builder paketobuildpacks/builder:base"
  },
  "cds": {
    "i18n": {
      "folders": [
        "_i18n"
      ]
    },
    "preview": {
      "ui5": {
        "version": "1.103.0"
      }
    },
    "cdsc": {
      "docs": true,
      "beta": {
        "sqlSnippets": true
      }
    },
    "hana": {
      "journal": {
        "enable-drop": false,
        "change-mode": "alter"
      },
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana"
      },
      "messaging": {
        "[production]": {
          "kind": "enterprise-messaging"
        },
        "[development]": {
          "kind": "file-based-messaging"
        },
        "[hybrid!]": {
          "kind": "enterprise-messaging-shared"
        }
      }
    },
    "features": {
      "preserve_computed": true,
      "kibana_formatter": true,
      "audit_personal_data": true,
      "optimized_search": true,
      "rest_new_adapter": true,
      "assert_integrity": "db",
      "odata_new_parser": true
    },
    "log": {
      "service": true
    },
    "plugins":[
      { "impl": "@sap/cds-graphql", "path": "/graphql"}
    ]
  },
  "sapux": [
    "app\\people"
  ],
  "devDependencies": {
    "@sap/hdi-deploy": "^4.4.1",
    "@sap/cds-dk": "6.0.1"
  }
}
